ton = {\n    background: \"none\",\n    fontFamily: \"Nunito Sans, sans-serif\",\n    margin: \"25px auto\",\n    textAlign: \"center\",\n    border: \"2px solid rgba(11, 228, 210, 0.533)\",\n    padding: \"10px 10px\",\n    width: \"250px\",\n    outline: \"none\",\n    color: \"white\",\n    borderRadius: \"24px\",\n    \n  }\n\n  // const removePost = id => {\n  //   API.deletePost(id)\n  //     .then(() => {\n  //       dispatch({\n  //         type: REMOVE_POST,\n  //         _id: id\n  //       });\n  //     })\n  //     .catch(err => console.log(err));\n  // };\n\n\n\n  return (\n    <>{state.currentPost ? (\n\n\n      <Container fluid>\n        <NavLogged />\n        <Row>\n          <div className=\"col-md-4 offset-md-4\">\n            <Jumbotron>\n              <h4 style={title}>{state.currentPost.title}</h4>\n              <h4 style={subtitle}>Location:</h4>\n              <h5 style={location}>{state.currentPost.where}</h5>\n              <h4 style={subtitle}>Person Amount:</h4>\n              <h5 style={amount}>{state.currentPost.total}</h5>\n              <h4 style={subtitle}>Description:</h4>\n              <h5 style={description}>{state.currentPost.description}</h5>\n            </Jumbotron>\n          </div>\n        </Row>\n        <Row>\n          <Col size=\"md-12\">\n            <Link style={button} to=\"/eventlist\">← Back to Events</Link>\n            {/* <DeleteBtn onClick={() => removePost(post._id)} /> */}\n\n          </Col>\n        </Row>\n      </Container>\n    ) : (\n        <div>loading...</div>\n      )}</>\n  );\n};\n\nexport default EventTarget;\n","/Users/genet/class_code/link-it/client/src/utils/GlobalState.js",[],"/Users/genet/class_code/link-it/client/src/utils/API.js",[],"/Users/genet/class_code/link-it/client/src/utils/actions.js",[],"/Users/genet/class_code/link-it/client/src/components/Navbar/index.js",[],"/Users/genet/class_code/link-it/client/src/components/CreateEvent/index.js",[],"/Users/genet/class_code/link-it/client/