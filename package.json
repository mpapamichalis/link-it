  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "boostrap": "^2.0.0",
    "bootstrap": "^4.6.0",
    "concurrently": "^6.0.0",
    "express": "^4.16.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.0.1",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "react-bootstrap": "^1.5.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  }
